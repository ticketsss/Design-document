<script type="text/javascript"> 
      function showHidCity(obj){ 
       if(obj.innerHTML == "[展开更多城市+]"){ 
        hids = document.getElementById("fcity_list").getElementsByTagName("a"); 
        for(i=0,len=hids.length;i<len;i++){ 
         if(hids[i].className=="hidcity"){ 
          hids[i].style.display = ""; 
         } 
        } 
        document.getElementById("fcity_list").style.cssText = "height:auto"; 
        obj.innerHTML = "[收起]"; 
       }else{ 
        hids = document.getElementById("fcity_list").getElementsByTagName("a"); 
        for(i=0,len=hids.length;i<len;i++){ 
         if(hids[i].className=="hidcity"){ 
          hids[i].style.display = "none"; 
         } 
        } 
        document.getElementById("fcity_list").style.cssText = ""; 
        obj.innerHTML = "[展开更多城市+]"; 
       } 
      } 
      </script>
